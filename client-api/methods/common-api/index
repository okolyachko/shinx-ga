        <span class="c1">/// Provide any params with json</span>
        <span class="kd">var</span> <span class="n">visualState</span> <span class="o">=</span> <span class="n">jsonEncode</span><span class="p">({</span><span class="s2">"visualState"</span><span class="o">:</span> <span class="s2">"stateValue"</span><span class="p">});</span>
        <span class="n">AlanVoice</span><span class="p">.</span><span class="n">setVisualState</span><span class="p">(</span><span class="n">visualState</span><span class="p">);</span>
